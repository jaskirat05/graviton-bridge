# Graviton Bridge local env example
# Copy to .env in this same folder and edit values.

# Used when bridge mode is "local" or "orchestrator"
GRAVITON_ORCHESTRATOR_BASE_URL=http://localhost:8001
GRAVITON_ORCHESTRATOR_TOKEN=

# Used when bridge mode is "s3"
GRAVITON_S3_BUCKET=
GRAVITON_S3_REGION=
GRAVITON_S3_ACCESS_KEY=
GRAVITON_S3_SECRET_KEY=
GRAVITON_S3_PREFIX=

# Used when bridge mode is "cloudinary"
GRAVITON_CLOUDINARY_CLOUD_NAME=
GRAVITON_CLOUDINARY_API_KEY=
GRAVITON_CLOUDINARY_API_SECRET=
GRAVITON_CLOUDINARY_FOLDER=
